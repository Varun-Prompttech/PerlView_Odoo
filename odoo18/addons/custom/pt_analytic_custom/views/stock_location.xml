<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_location_add_analytic" model="ir.ui.view">
        <field name="name">stock_location_add_analytic</field>
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="stock.view_location_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='storage_category_id']" position="after">
                <field name="analytic_account_id" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>