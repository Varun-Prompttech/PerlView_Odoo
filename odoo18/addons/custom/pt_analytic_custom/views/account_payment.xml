<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="account_payment_add_analytic" model="ir.ui.view">
        <field name="name">account_payment_add_analytic</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='journal_id']" position="after">
                <field name="analytic_account_id" widget="many2many_tags" readonly="state != 'draft'"/>
            </xpath>
        </field>
    </record>
</odoo>