<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="inherit_res_partner_form" model="ir.ui.view">
        <field name="name">inherit_res_partner_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="without_tax"/>
            </xpath>
            <xpath expr="//field[@name='property_product_pricelist']" position="after">
                <field name="purchase_taxes_id" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>