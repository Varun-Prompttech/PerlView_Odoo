<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sale_order_add_analytic" model="ir.ui.view">
        <field name="name">sale_order_add_analytic</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_order']" position="after">
                <field name="analytic_account_id" widget="many2many_tags" required="1" readonly="state=='sale'"/>
            </xpath>
        </field>
    </record>

</odoo>