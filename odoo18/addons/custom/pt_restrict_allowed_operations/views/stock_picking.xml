<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="stock_form_remove_domain" model="ir.ui.view">
    <field name="name">stock_form_remove_domain</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"/>  <!-- Inherit the existing view -->
    <field name="arch" type="xml">
        <!-- Override the field by removing the domain -->
        <field name="picking_type_id" position="replace">
                <field name="picking_type_id" options="{'no_create': True}" readonly="state in ['cancel', 'done']"/>
        </field>
    </field>
</record>

</odoo>