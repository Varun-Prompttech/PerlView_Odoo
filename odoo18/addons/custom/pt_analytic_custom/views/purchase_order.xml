<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="purchase_order_add_analytic" model="ir.ui.view">
        <field name="name">purchase_order_add_analytic</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_order']" position="after">
                <field name="analytic_account_id" widget="many2many_tags" required="1" readonly="state=='purchase'"/>
            </xpath>
        </field>
    </record>
</odoo>